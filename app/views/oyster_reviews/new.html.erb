    <div class="container">
      <h3>
        Post a Review 
      </h3>
      <p>
        The more we share, the more we have. 
      </p>
<h5>Welcome, <%= current_user.first_name %></h5>
<%= form_tag '/oysterreviews', method: :post do %>
<div class="form-group">
  <label for="exampleInputName">Review Title</label>
  <input type="text" name="title" class="form-control" id="exampleInputName" placeholder="Title">
</div>
<div class="form-group">
  <label for="exampleInputName">Review</label>
  <input type="text" name="body" class="form-control" id="exampleInputName" placeholder="Body">
</div>
<div class="form-group">
<label for="exampleInputName">Oyster Quality</label>
  <select name="oyster_quality">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">4</option>
    <option value="5">5</option>
    <option value="6">6</option>
    <option value="7">7</option>
    <option value="8">8</option> 
    <option value="9">9</option>
    <option value="10">10</option>
  </select>
</div>
<div class="form-group">
<label for="exampleInputName">Shucking Quality</label>
  <select name="shucking_quality">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">4</option>
    <option value="5">5</option>
    <option value="6">6</option>
    <option value="7">7</option>
    <option value="8">8</option>
    <option value="9">9</option>
    <option value="10">10</option>
  </select>
</div>
<div class="form-group">
<label for="exampleInputName">Presentation</label>
  <select name="presentation">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">4</option>
    <option value="5">5</option>
    <option value="6">6</option>
    <option value="7">7</option>
    <option value="8">8</option>
    <option value="9">9</option>
    <option value="10">10</option>
  </select>
</div>
<div class="form-group">
<label for="exampleInputName">Restaurent Name:</label> <input type="text" name="name" class="form-control" placeholder="Restaurent Name">
</div>
<div class="form-group">
  <lable for="exampleInputName">Oyster Name: </lable>
  <%= select_tag "oyster",options_from_collection_for_select(@oysters, "id", "oyster_type") %>
</div>
<div class="form-action">
  <button type="submit" class="btn-shadow btn-shadow-dark">Post</button>
</div>
<% end %> 
